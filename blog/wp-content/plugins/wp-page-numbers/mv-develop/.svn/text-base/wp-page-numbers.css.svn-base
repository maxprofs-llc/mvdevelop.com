#wp_page_numbers ul, #wp_page_numbers li, #wp_page_numbers a {
	background: #fff;
	padding: 0;
	margin: 0;
	border: none;
	text-decoration: none;
	font-weight: normal;
	font-style: normal;
	list-style: none;
	text-transform: none;
	text-indent: 0px;
	font-variant: normal;
	text-align: left;
	line-height: 12px;
	letter-spacing: 0px;
	word-spacing: 0px;
}

#wp_page_numbers {
	margin-top: 30px;
	width: 100%;
	background: #fff;
}
#wp_page_numbers ul {
	width: 100%;
}

#wp_page_numbers li {
	float: left;
	display: block;
	margin-right: 6px;
}

#wp_page_numbers a {
	padding: 4px 10px 4px 10px;
	display: block;
	color: #333;
	font-size: 14px;
	line-height: 18px;
	border-top: 2px solid #fff;
}

#wp_page_numbers li a:hover {
	color: #f60;
	border-top: 2px solid #f60;
	font-weight: bold;
}

#wp_page_numbers li.active_page a {
	color: #fff;
	background: #00a5ff;
	border-top: 2px solid #00a5ff;
	border-radius: 4px;
	font-weight: bold;
}

#wp_page_numbers li.page_info {
	float: left;
	font-size: 12px;
	color: #aaa;
	display: block;
	padding: 9px 10px 0 0;
}